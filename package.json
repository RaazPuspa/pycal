{
  "name": "pycal-frontend",
  "version": "1.0.0",
  "description": "The reactJS front-end for PyCal",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev && python3 manage.py runserver",
    "setup": "npm install && python3 -m pip install -r requirement.txt",
    "dev": "webpack --mode development ./frontend/src/index.js --output ./frontend/static/main.js",
    "build": "webpack --mode production ./frontend/src/index.js --output ./frontend/static/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "axios": "^0.21.1",
    "babel-loader": "^8.0.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
